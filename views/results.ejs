<h1> GREAT !</h1>


<form action="/results" method="GET">
    <input type="text" placeholder="Movie title" name="search">
    <input type="submit">
</form>

<% data["Search"].forEach(function(movie){ %>

    <li>
        <%= movie["Title"] %>
        <img src="<%= movie["Poster"] %>" >
    </li>

<% }) %>